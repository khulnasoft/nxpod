{
  "server": {
    "port": 3000,
    "counterMetrics": [
      {
        "name": "nxpod_test_counter",
        "help": "help",
        "labels": [
          {
            "name": "ide",
            "allowValues": ["vscode", "idea", "goland", "pycharm"],
            "defaultValue": "intellij"
          }
        ],
        "client": {
          "name": "metric_client",
          "allowValues": ["vscode", "supervisor"],
          "defaultValue": "supervisor"
        }
      },
      {
        "name": "nxpod_test_another_counter",
        "help": "help",
        "labels": [
          {
            "name": "ide",
            "allowValues": ["vscode", "idea", "goland", "pycharm"],
            "defaultValue": "intellij"
          }
        ]
      }
    ],
    "histogramMetrics": [
      {
        "name": "nxpod_test_hist",
        "help": "hist_help",
        "labels": [
          {
            "name": "ide",
            "allowValues": ["idea", "goland", "pycharm"]
          }
        ],
        "buckets": [1, 10, 100],
        "client": {
          "name": "metric_client",
          "allowValues": ["jetbrains"],
          "defaultValue": "jetbrains"
        }
      }
    ],
    "errorReporting": {
      "allowComponents": ["supervisor-frontend"]
    }
  },
  "prometheus": {
    "addr": "127.0.0.1:9500"
  }
}
