{
  "dockerAuth": "/home/nxpod/.docker/config.json",
  "registry": {
    "port": 9090,
    "staticLayer": [
      {
        "type": "file",
        "ref": "example-layer.tar.gz"
      }
    ],
    "store": "/tmp/store",
    "requireAuth": false,
    "fixedSpecFN": "example-spec.json",
    "ipfs": {
      "enabled": true,
      "redis": {
        "singleHostAddr": "localhost:6379"
      },
      "ipfs": "/ip4/127.0.0.1/tcp/5001"
    }
  },
  "blobserve": {
    "port": 8081,
    "timeout": "5s",
    "repos": {
      "eu.gcr.io/nxpod-core-dev/build/theia-ide": {
        "prePull": ["master.28"],
        "workdir": "/theia/node_modules/@nxpod/nxpod-ide/lib"
      }
    },
    "blobSpace": {
      "location": "/tmp/bs",
      "maxSizeBytes": 44631060
    }
  },
  "pprofAddr": ":6060"
}