{
    "defaultTemplate": {
        "spec": {
            "dnsConfig": {
                "nameservers": [
                    "1.1.1.1",
                    "8.8.8.8"
                ]
            },
            "dnsPolicy": "None",
            "imagePullSecrets": [
                {
                    "name": "dockerhub-typefox"
                },
                {
                    "name": "eu.gcr.io-nxpod"
                }
            ],
            "affinity": {
                "nodeAffinity": {
                    "requiredDuringSchedulingIgnoredDuringExecution": {
                        "nodeSelectorTerms": [
                            {
                                "matchExpressions": [
                                    {
                                        "key": "nxpod.io/workload_workspace",
                                        "operator": "In",
                                        "values": [
                                            "true"
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                }
            },
            "containers": [
              {
                "name": "workspace",
                "env": [
                  { "name": "some-envvar", "value": "foofoo" }
                ]
              }
            ]
        }
    },
    "spec": {
        "theiaVersion": "foobar",
        "workspaceImage": "eu.gcr.io/nxpod-dev/workspace-images/ac1c0755007966e4d6e090ea821729ac747d22ac/eu.gcr.io/nxpod-dev/workspace-base-images/github.com/typefox/nxpod:80a7d427a1fcd346d420603d80a31d57cf75a7af",
        "initializer": {
            "snapshot": {
                "snapshot": "workspaces/cryptic-id-goes-herg/fd62804b-4cab-11e9-843a-4e645373048e.tar@nxpod-dev-user-christesting"
            }
        },
        "ports": [
            {
                "port": 8080,
                "target": 38080
            }
        ],
        "envvars": [
            {
                "name": "foo",
                "value": "bar"
            }
        ],
        "git": {
            "username": "usernameGoesHere",
            "email": "some@user.com"
        }
    }
}